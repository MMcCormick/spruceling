#box-show

  %p
    %b Gender:
    = @box.gender
  %p
    %b Size:
    = @box.size

  .photo-list
    - @box.photos.each do |photo|
      = render "photos/cover_photo", :photo => photo

  = render 'boxes/itemized', :box => @box

  = link_to 'Edit', edit_box_path(@box)
  \|
  = link_to 'Back', boxes_path
