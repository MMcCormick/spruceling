#my_orders
  %h1 Completed Orders

  .section
    %table
      %tr
        %th Order Number
        %th Date
        %th Number of Boxes
        %th Total Price
        %th

      - @orders.each do |order|
        %tr
          %td= "Order ##{order.id}"
          %td= "#{order.created_at.strftime('%m/%d/%Y')}"
          %td= "#{order.boxes.length}"
          %td= "$#{order.price_total}"
          %td= link_to 'View Confirmation', order